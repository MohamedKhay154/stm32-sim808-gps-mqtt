    {
        "id": "c252b7f2d1866318",
        "type": "function",
        "z": "b79d78232292bda9",
        "name": "Park",
        "func": "// ===== PARSE LAT/LON ===== \nlet parts = msg.payload.split(\",\"); \nlet lat = parseFloat(parts[0]); \nlet lon = parseFloat(parts[1]); \n// ===== PARK SETTINGS ===== \nconst PARK_RADIUS_M = 20; \n// Save park center for later use (geofence check, redraw) \ncontext.set(\"park_lat\", lat); \ncontext.set(\"park_lon\", lon); \ncontext.set(\"park_radius\", PARK_RADIUS_M); \n// ===== GEOFENCE CIRCLE ===== \nlet circleMsg = { \n    payload: {\n         name: \"ParkZone\", \n         lat: lat, \n         lon: lon, \n         radius: PARK_RADIUS_M, \n         color: \"red\", \n         fillColor: \"red\", \n         fillOpacity: 0.2, \n         layer: \"layer1\" \n         } \n}; \nreturn circleMsg;\n",
        "outputs": 2,
        "timeout": 0,
