        "id": "f293c9140278359c",
        "type": "function",
        "z": "b79d78232292bda9",
        "name": "check connection",
        "func": "// Check if we have received any message yet\nlet connected = global.get(\"mqttConnected\") || false;\n\n// If this message arrived, mark as connected\nconnected = true;\nglobal.set(\"mqttConnected\", connected);\nflow.set(\"lastMqttTime\", Date.now());\n// Prepare LED message\nmsg.payload = connected; // true → green\nreturn msg;\n",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
